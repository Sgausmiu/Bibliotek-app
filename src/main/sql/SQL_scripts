--Создение таблиц БД
CREATE table Person(
    id int GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    full_name varchar(200) NOT NULL UNIQUE,
    year_birth int NOT NULL
);

CREATE TABLE Book (
    id int GENERATED BY DEFAULT AS IDENTITY primary key,
    named varchar(200) NOT NULL UNIQUE,
    author varchar(200) NOT NULL,
    yearBook int NOT NULL
);